{
  "name": "blog-app",
  "version": "1.0.0",
  "description": "Blog App",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon src/index",
    "semistandard": "semistandard --fix",
    "tap": "c8 --lines 80 --functions 80 --branches 80 --statements 80 --check-coverage --per-file tap test/**/*.test.js test/**/**/*.test.js --no-coverage",
    "test": "npm run semistandard && npm run tap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasrelperalta/blog-app.git"
  },
  "keywords": [
    "Node",
    "JS",
    "Web",
    "Server"
  ],
  "author": "Jasrel Peralta",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jasrelperalta/blog-app/issues"
  },
  "homepage": "https://github.com/jasrelperalta/blog-app#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "c8": "^7.12.0",
    "chance": "^1.1.9",
    "eslint": "8.19.0",
    "eslint-config-semistandard": "16.0.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-chai-expect": "3.0.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-standard": "4.1.0",
    "must": "^0.13.4",
    "nodemon": "^2.0.20",
    "semistandard": "^16.0.1",
    "tap": "^16.3.0"
  },
  "semistandard": {
    "plugins": [
      "html",
      "standard",
      "promise",
      "mocha",
      "chai-expect"
    ],
    "parser": "@babel/eslint-parser",
    "env": [
      "mocha"
    ],
    "globals": [
      "describe",
      "it",
      "after",
      "before",
      "expect"
    ]
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/jwt": "^6.5.0",
    "@fastify/secure-session": "^5.3.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/swagger": "7.6.1",
    "bcrypt": "^5.1.0",
    "fastify": "^4.11.0",
    "fastify-openapi-glue": "^4.1.1",
    "uuid": "^9.0.0"
  }
}
